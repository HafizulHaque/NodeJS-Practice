<!DOCTYPE html>
<html>
	<head>
		<title>Movie List</title>
		<style>
			img{
				height:150px;
				width:100px;
				margin:20px;
				padding:20px;
				border:2px solid #ddd;
			}
		</style>
	</head>
	<body>
		<% movies.forEach(function(movie){ %>
			<img src="<%= movie['Poster'] %>" />
		<% }); %>
	</body>
</html>